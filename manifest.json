{
  "name": "CF Helper",
  "description": "Developed By Rahul Indra",
  "version": "1.0",
  "permissions": [
  "tabs",
  "unlimitedStorage"
],
  "content_scripts" :[
  	{

  		"matches": ["http://codeforces.com/*"],
  		"js" : ["Codeforces.js"]


  	}
